{
  "ConnectionStrings": {
    "ServiceBusConnectionString": "Endpoint=sb://devpfm-k3y-sbx-sbus.servicebus.windows.net/;Authentication=ManagedIdentity",
    "EventHubConnectionString": "devpfm-k3y-sbx-evhn.servicebus.windows.net",
    "ProcessManagerDbConnectionString": ""
  },
  "Settings": {
    "Resource": "devpfm-k3y-sbx-rg"
  },
  "ProcessManagerConfiguration": {
    "ServiceBusQueueName": "processes",
    "EventHubName": "workflows",
    "ConfigurationUri": "/api/ReferenceData/workflowSettings",
    "ConfigurationManagerBaseUrl": "https://devpfm-k3y-configurationmanagerapi.azurewebsites.net/",
    "EventGridTopicEndpoint": "https://devpfm-k3y-sbx-event-collector.westeurope-1.eventgrid.azure.net/api/events",
    "SendEventsDelayInSec": 2,
    "StartLogicAppsDelayInSeconds": 1,
    "DefaultAbsoluteExpirationInSeconds": 600,
    "DefaultSlidingExpirationInSeconds": 600,
    "StartLogicAppMaxRetryNumber": 2,
    "StartLogicAppInitialDelayInSec": 1
  },
  "ResourceLevel": "env",
  "AuthorizationConfiguration": {
    "AccessRoleServiceEndpoint": "https://devpfm-k3y-accessrolessvc.azurewebsites.net/",
    "ClaimScopes": "ProcessManager",
    "EnvironmentName": "Sandbox"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": "Verbose",
    "Override": {
      "Microsoft": "Information",
      "System": "Warning"
    },
    "WriteTo": [
      {
        "Name": "ApplicationInsights",
        "Args": {
          "restrictedToMinimumLevel": "Information",
          "telemetryConverter": "Serilog.Sinks.ApplicationInsights.Sinks.ApplicationInsights.TelemetryConverters.TraceTelemetryConverter, Serilog.Sinks.ApplicationInsights"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  }
}